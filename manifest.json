{
    "manifest_version": 2,
    "name": "Conversion",
    "version": "1.0",
    "description": "Turn imperial to metric!",
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["jquery.min.js", "./main.js"],
            "css": ["./index.css"]
        }
    ],
    "icons": {
        "16": "favicon-16x16.png",
        "32": "favicon-32x32.png"
    }
}